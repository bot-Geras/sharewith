{
    "plugins": [
        [
            "module:react-native-dotenv",
            {
                "envName": "APP_ENV",
                "moduleName": "@env",
                "path": ".env",
                "blocklist": null,
                "allowlist": null,
                "safe": true,
                "allowUndefined": true,
                "verbose": false
            }
        ]
    ]
}